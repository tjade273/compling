  author = {Feng, D. F. and Doolittle, R. F.},
  year = {1987},
  keywords = {Algorithms,Amino Acid Sequence,Animals,Biological Evolution,Hemoglobins,Humans,Methods,Phylogeny,Proteins,Protein-Tyrosine Kinases,Superoxide Dismutase},
