  file = {Brill Journals PDF Full Text:/home/tjaden/.zotero/zotero/5omcrhwv.default/zotero/storage/62SZ72ZE/Steiner et al. - 2011 - A Pipeline for Computational Historical Linguistic.pdf:application/pdf;Snapshot:/home/tjaden/.zotero/zotero/5omcrhwv.default/zotero/storage/37UV47DD/221058211x570358.html:text/html}
